<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="CodigosClasePU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>proyecto.biblioteca.Entity.Bibliotecas</class>
    <class>proyecto.biblioteca.Entity.Empleados</class>
    <class>proyecto.biblioteca.Entity.Autores</class>
    <class>proyecto.biblioteca.Entity.TipoUsuario</class>
    <class>proyecto.biblioteca.Entity.Categorias</class>
    <class>proyecto.biblioteca.Entity.Materiales</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/biblioteca?zeroDateTimeBehavior=CONVERT_TO_NULL"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.cj.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="Admin123"/>
    </properties>
  </persistence-unit>
</persistence>
